{
  "name": "cy-boilerplate",
  "version": "1.0.0",
  "description": "A boilerplate cypress framework to explore its capabilities",
  "main": "index.js",
  "scripts": {
    "lint": "npx eslint cypress/e2e/**/*.cy.ts",
    "lint:fix": "npx eslint --fix cypress/e2e/**/*.cy.ts",
    "prettier": "prettier cypress/e2e/**/*.cy.ts --write",
    "cy:open": "npx cypress open",
    "cy:run:report": "cypress run --spec cypress/e2e/**/** --browser chrome",
    "cy:run:api:report": "cypress run --spec cypress/e2e/api/** --browser chrome"
  },
  "keywords": [],
  "author": "Eduardo Lopez",
  "license": "ISC",
  "dependencies": {
    "cypress": "^13.7.2"
  },
  "devDependencies": {
    "ajv": "^8.12.0",
    "cypress-mochawesome-reporter": "^3.8.2",
    "cypress-plugin-api": "^2.11.1",
    "eslint": "^8.57.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-cypress": "^2.15.1",
    "prettier": "^3.2.5",
    "typescript": "^5.4.3"
  }
}