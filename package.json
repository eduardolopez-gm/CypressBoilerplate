{
  "name": "cy-boilerplate",
  "version": "1.0.0",
  "description": "A boilerplate cypress framework to explore its capabilities",
  "main": "index.js",
  "scripts": {
    "lint": "npx eslint cypress/e2e/**/*.cy.js",
    "lint:fix": "npx eslint --fix cypress/e2e/**/*.cy.js",
    "cy:open": "npx cypress open",
    "cy:run:cucumber": "cypress run --spec **/*.feature || node cucumber-html-report.js"
  },
  "keywords": [],
  "author": "Eduardo Lopez",
  "license": "ISC",
  "dependencies": {
    "cypress": "^13.7.0"
  },
  "devDependencies": {
    "cypress-cucumber-preprocessor": "^4.3.1",
    "eslint": "^8.57.0",
    "eslint-config-google": "^0.14.0",
    "eslint-plugin-cypress": "^2.15.1",
    "multiple-cucumber-html-reporter": "^3.6.2"
  },
  "cypress-cucumber-preprocessor": {
    "nonGlobalStepDefinitions": false,
    "stepDefinitions": "cypress/e2e/step-definitions",
    "cucumberJson":{
      "generate":true,
      "outputFolder": "cypress/cucumber-json",
      "filePrefix": "",
      "fileSuffix": ".cucumber"
    }
  }
}
